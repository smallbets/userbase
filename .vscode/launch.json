  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Encrypted.dev",
      "sourceMaps": true,
      "program": "${workspaceFolder}/src/server/server.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "stopOnEntry": false,
      "outFiles": [
        "${workspaceRoot}/build/**"
      ]
    }
  ]
}
